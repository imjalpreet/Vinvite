<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light">
            <ImageButton
                android:layout_width="72dp"
                android:layout_height="70dp"
                android:background="@null"
                android:id="@+id/imageButton5"
                android:layout_above="@+id/imageButton9"
                android:layout_toRightOf="@+id/imageButton7"
                android:layout_toEndOf="@+id/imageButton7"></ImageButton>
            <ImageButton
                android:layout_width="72dp"
                android:layout_height="70dp"
                android:background="@null"
                android:id="@+id/imageButton6"
                android:layout_gravity="center_horizontal"
                android:layout_toEndOf="@+id/imageButton7"
                android:layout_below="@+id/imageButton5"
                android:layout_alignLeft="@+id/imageButton5"
                android:layout_alignStart="@+id/imageButton5"></ImageButton>

            <ImageButton
                android:layout_width="72dp"
                android:layout_height="70dp"
                android:background="@null"
                android:id="@+id/imageButton7"
                android:layout_gravity="center_horizontal"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"></ImageButton>

    <ImageButton
                android:layout_width="72dp"
                android:layout_height="70dp"
                android:background="@null"
                android:id="@+id/imageButton8"
                android:layout_gravity="center_horizontal"
                android:layout_toEndOf="@+id/imageButton5"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton5"></ImageButton>

    <ImageButton
        android:layout_width="72dp"
        android:layout_height="70dp"
        android:background="@null"
        android:id="@+id/imageButton9"
        android:layout_gravity="center_horizontal"
        android:layout_below="@+id/imageButton7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"></ImageButton>

    <ImageButton
        android:layout_width="72dp"
        android:layout_height="70dp"
        android:background="@drawable/back"
        android:id="@+id/ibBack"
        android:layout_toEndOf="@+id/imageButton5"
        android:layout_below="@+id/imageButton5"
        android:layout_toRightOf="@+id/imageButton5"></ImageButton>

</RelativeLayout>